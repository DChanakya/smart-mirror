{
    "schema": {
        "stock": {
            "type": "object",
            "title": "{{title}}",
            "properties": {
                "key": {
                    "type": "string",
                    "title": "{{api key}}"
                },
                "names": {
                    "type": "array",
                    "title": "{{Stock Symbols}}",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "form": [
        {
            "type": "fieldset",
            "title": "{{title}}",
            "expandable": true,
            "order":0,
            "items":[
              "stock.key",
              {
                "type":"tabarray",
                "title":"{{Stock Symbols}}",
                "items":[
                    {
                      "type":"section",
                      "legend":"{{value}}",
                      "items": [
                        {
                          "key":"stock.names[]",
                          "notitle":true,
                          "valueInLegend":true
                        }
                      ]
                    }  
                ]
              }
          ]
        }
    ]
}